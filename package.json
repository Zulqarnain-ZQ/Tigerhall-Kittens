{
  "name": "tigerhall-kittens",
  "version": "1.0.0",
  "description": "Probject about tigers and their sight seeing",
  "main": "index.ts",
  "scripts": {
    "test": "env-cmd -f ./config/.env.test jest --runInBand",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "dev": "env-cmd -f ./config/.env.dev nodemon src/index.ts -e ts",
    "build": "npm-run-all clean tsc",
    "watch": "tsc-watch --onSuccess \"node dist/index\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "author": "Muhammad Zulqarnain",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "^1.7.2",
    "apollo-server-express": "2.25.2",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "graphql": "15.5.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-upload": "^13.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.3",
    "type-graphql": "^1.1.1",
    "typedi": "0.8.0",
    "typeorm": "0.2.37",
    "typeorm-typedi-extensions": "0.2.3",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.4.1",
    "@types/sharp": "^0.30.1",
    "@types/uuid": "^8.3.4",
    "apollo-server-testing": "^2.25.3",
    "env-cmd": "^10.1.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsc": "^2.0.4",
    "tsc-watch": "^4.6.2",
    "tsconfig-paths": "^3.14.1"
  }
}
